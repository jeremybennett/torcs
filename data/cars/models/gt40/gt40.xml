    <attnum name="rim diameter" unit="in" val="19.0"/>
    <attnum name="tire width" unit="mm" val="285"/>
    <attnum name="tire height-width ratio" val=".45"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>

