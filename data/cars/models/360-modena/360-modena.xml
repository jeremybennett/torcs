<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Ferrari 360 Modena" type="template">
  <section name="Driver">

    <!-- Position of the driver -->
    <attnum name="xpos" val="0.75" unit="m"/>
