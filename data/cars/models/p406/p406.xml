<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Peugeot 406" type="template">

    
  <section name="Graphic Objects">
    <attstr name="wheel texture" val="tex-wheel.rgb"/>
    <attstr name="shadow texture" val="shadow.rgb"/>
    <attstr name="tachometer texture" val="rpm7000.rgb"/>
    <attnum name="tachometer min value" unit="rpm" val="0"/>
    <attnum name="tachometer max value" unit="rpm" val="8000"/>
    <attstr name="speedometer texture" val="speed240.rgb"/>
    <attnum name="speedometer min value" unit="km/h" val="0"/>
    <attnum name="speedometer max value" unit="km/h" val="300"/>

    <section name="Ranges">
      <section name="1">
	<attnum name="threshold" val="50"/>
	<attstr name="car" val="p406.acc"/>
      </section>
      <section name="2">
	<attnum name="threshold" val="40"/>
	<attstr name="car" val="p406-lod1.acc"/>
	<attstr name="wheels" val="yes"/>
      </section>
      <section name="3">
	<attnum name="threshold" val="30"/>
	<attstr name="car" val="p406-lod2.acc"/>
	<attstr name="wheels" val="yes"/>
      </section>
      <section name="4">
	<attnum name="threshold" val="20"/>
	<attstr name="car" val="p406-lod3.acc"/>
	<attstr name="wheels" val="yes"/>
      </section>
      <section name="5">
	<attnum name="threshold" val="10"/>
	<attstr name="car" val="p406-lod4.acc"/>
	<attstr name="wheels" val="yes"/>
      </section>
      <section name="6">
	<attnum name="threshold" val="5"/>
	<attstr name="car" val="p406-lod5.acc"/>
      </section>
      <section name="7">
	<attnum name="threshold" val="3"/>
	<attstr name="car" val="p406-lod6.acc"/>
      </section>
      <section name="8">
	<attnum name="threshold" val="0"/>
	<attstr name="car" val="p406-lod7.acc"/>
      </section>
    </section>
  </section>

  <section name="Driver">
    <!-- Position of the driver -->
    <attnum name="xpos" unit="m" val="0.8"/>
    <attnum name="ypos" unit="m" val="0.0"/>
    <attnum name="zpos" unit="m" val="1.1"/>
  </section>

  <section name="Car">
    <attstr name="category" val="Track-FWD-GrB"/>    
    <attnum name="body length" min="4.5" max="5.0" unit="m" val="4.64"/>
    <attnum name="body width" min="0.8" max="2.0" unit="m" val="2.0"/>
    <attnum name="body height" min="1.1" max="3.0" unit="m" val="1.27"/>
    <!-- collision bounding box -->
    <attnum name="overall length" min="4.5" max="5.0" unit="m" val="4.64"/>
    <attnum name="overall width" min="1.2" max="2.0" unit="m" val="2.0"/>
    <attnum name="mass" min="600.0" max="2500" unit="kg" val="1200.0"/>
    <attnum name="GC height" unit="m" val="0.20"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.7" val="0.65"/>
    <attnum name="front right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.3" max="0.7" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="mass repartition coefficient" val="0.7"/>
    <attnum name="fuel tank" min="30.0" max="100.0" unit="l" val="100.0"/>
    <attnum name="initial fuel" min="1.0" max="100.0" unit="l" val="100.0"/>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="2.0" val="0.35"/>
    <attnum name="front area" min="1.0" max="5.0" unit="m2" val="2.085"/>
    <attnum name="front Clift" val="0"/>
    <attnum name="rear Clift" val="0.1"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area" unit="m2" val="0"/>
  </section>

  <!-- Same engine for every one -->

  <section name="Engine">
    <attnum name="inertia" min="0.1" max="1.0" unit="kg.m2" val="1.0"/>
    <attnum name="revs limiter" min="6000" max="8500" unit="rpm" val="6500"/>
  </section>


  <section name="Gearbox">
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-3" max="0" val="-2.0"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="3.82"/>
	<attnum name="inertia" val="0.003"/>
	<attnum name="efficiency" val="0.955"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="2.15"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" val="0.957"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.56"/>
	<attnum name="inertia" val="0.0048"/>
	<attnum name="efficiency" val="0.950"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="1.21"/>
	<attnum name="inertia" val="0.0064"/>
	<attnum name="efficiency" val="0.983"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.97"/>
	<attnum name="inertia" val="0.0107"/>
	<attnum name="efficiency" val="0.948"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.75"/>
	<attnum name="inertia" val="0.0150"/>
	<attnum name="efficiency" val="0.940"/>
      </section>
    </section>
  </section>

  <section name="Steer">
    <attnum name="steer lock" min="10" max="45" unit="deg" val="45"/>
    <attnum name="max steer speed" min="1" max="360" unit="deg/s" val="180"/>
  </section>


  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type" val="FWD"/>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.70"/>
    <attnum name="max pressure" min="1000" max="500000" unit="kPa" val="30000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" unit="m" val="1.37"/>
    <attnum name="inertia" unit="kg.m2" val="0.0056"/>
    <attnum name="roll center height" unit="m" val="0.5"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" unit="m" val="-1.37"/>
    <attnum name="inertia" unit="kg.m2" val="0.0080"/>
    <attnum name="roll center height" unit="m" val="0.4"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" val="NONE"/>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="FREE"/>
    <attnum name="inertia" unit="kg.m2" val="0.08"/>
    <attnum name="ratio" min="0" max="10" val="3.444"/>
    <attnum name="efficiency" val="0.9625"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" val="-0.75"/>
    <attnum name="rim diameter" min="15" max="17" unit="in" val="16.0"/>
    <attnum name="tire width" unit="mm" val="205"/>
    <attnum name="tire height-width ratio" val=".55"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val=".9"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <!-- initial ride height -->
    <attnum name="ride height" min="50" max="1000" unit="mm" val="150"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
    <attnum name="stiffness" val="5.0"/>
    <attnum name="dynamic friction" unit="%" val="100"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" val="0.75"/>
    <attnum name="rim diameter" min="15" max="17" unit="in" val="16.0"/>
    <attnum name="tire width" unit="mm" val="205"/>
    <attnum name="tire height-width ratio" val=".55"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val=".9"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
    <attnum name="ride height" min="50" max="1000" unit="mm" val="150"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="0"/>
    <attnum name="stiffness" val="5.0"/>
    <attnum name="dynamic friction" unit="%" val="100"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" val="-0.78"/>
    <attnum name="rim diameter" min="15" max="17" unit="in" val="16.0"/>
    <attnum name="tire width" unit="mm" val="205"/>
    <attnum name="tire height-width ratio" val=".55"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val="1.0"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="ride height" min="50" max="1000" unit="mm" val="150"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
    <attnum name="stiffness" val="5.0"/>
    <attnum name="dynamic friction" unit="%" val="100"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ypos" unit="m" val="0.78"/>
    <attnum name="rim diameter" min="15" max="17" unit="in" val="16.0"/>
    <attnum name="tire width" unit="mm" val="205"/>
    <attnum name="tire height-width ratio" val=".55"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200"/>
    <attnum name="mu" min="0.05" max="1.4" val="1.0"/>
    <attnum name="rolling resistance" val="0.0105"/>
    <attnum name="ride height" min="50" max="1000" unit="mm" val="150"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-2"/>
    <attnum name="stiffness" val="5.0"/>
    <attnum name="dynamic friction" unit="%" val="100"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="500"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="0"/>
    <attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    <attnum name="bellcrank" min="1" max="5" val="2.5"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
    <attnum name="suspension course" min="0" max="0.5" unit="m" val="0.3"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="100"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
    <attnum name="suspension course" min="0" max="0.5" unit="m" val="0.3"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="100"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="100"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
    <attnum name="suspension course" min="0" max="0.5" unit="m" val="0.3"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="70"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="70"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="80"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" min="0" max="10000" unit="lbs/in" val="2000"/>
    <attnum name="suspension course" min="0" max="0.5" unit="m" val="0.3"/>
    <attnum name="bellcrank" min="0.1" max="5" val="1.2"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="70"/>
    <attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="70"/>
    <attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="80"/>
    <attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="80"/>
  </section>

  <section name="Front Right Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="280"/>
    <attnum name="piston area" unit="cm2" val="25"/>
    <attnum name="mu" val="0.30"/>
    <attnum name="inertia" unit="kg.m2" val="0.1441"/>
  </section>

  <section name="Front Left Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="280"/>
    <attnum name="piston area" unit="cm2" val="25"/>
    <attnum name="mu" val="0.30"/>
    <attnum name="inertia" unit="kg.m2" val="0.1441"/>
  </section>

  <section name="Rear Right Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="120"/>
    <attnum name="piston area" unit="cm2" val="8"/>
    <attnum name="mu" val="0.30"/>
    <attnum name="inertia" unit="kg.m2" val="0.1014"/>
  </section>

  <section name="Rear Left Brake">
    <attnum name="disk diameter" min="100" max="380" unit="mm" val="120"/>
    <attnum name="piston area" unit="cm2" val="8"/>
    <attnum name="mu" val="0.30"/>
    <attnum name="inertia" unit="kg.m2" val="0.1014"/>
  </section>

</params>


