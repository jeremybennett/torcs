      <section name="3">
        <attnum name="threshold" val="0"/>
        <attstr name="car" val="mclaren-f1-lod2.acc"/>
      </section>
    </section>

    <section name="Light">
      <section name="1">
	<attstr name="type" val="brake2"/>
	<attnum name="xpos" val="-2.20"/>
	<attnum name="ypos" val="0.41"/>
	<attnum name="zpos" val="0.61"/>
	<attnum name="size" val="0.3"/>
      </section>
      <section name="2">
	<attstr name="type" val="brake2"/>
	<attnum name="xpos" val="-2.20"/>
	<attnum name="ypos" val="-0.41"/>
	<attnum name="zpos" val="0.61"/>
	<attnum name="size" val="0.3"/>
      </section>
    </section>

  </section>

  <section name="Car">
    <attstr name="category" val="Track-RWD-GrA"/>
    <attnum name="body length" unit="m" min="4.5" max="5.0" val="4.41"/>
    <attnum name="body width" unit="m" min="0.8" max="2.0" val="2.0"/>
    <attnum name="body height" unit="m" min="1.1" max="1.7" val="1.1"/>

