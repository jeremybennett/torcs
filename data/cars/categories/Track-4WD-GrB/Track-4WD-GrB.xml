  <section name="Clutch">
    <!-- pressure plate -->
    <attnum name="inertia" unit="kg.m2" val="0.0150"/>
  </section>

  <section name="Gearbox">
    <attnum name="shift time" min=".10" max="0.30" val="0.1" unit="s"/>
    <section name="gears">
      <section name="r">
	<attnum name="ratio" min="-5" max="0" val="-2.0"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.957"/>
      </section>
      <section name="1">
	<attnum name="ratio" min="0" max="5" val="2.9"/>
	<attnum name="inertia" val="0.003"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.955"/>
      </section>
      <section name="2">
	<attnum name="ratio" min="0" max="5" val="1.76"/>
	<attnum name="inertia" val="0.0037"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.957"/>
      </section>
      <section name="3">
	<attnum name="ratio" min="0" max="5" val="1.1"/>
	<attnum name="inertia" val="0.0048"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.950"/>
      </section>
      <section name="4">
	<attnum name="ratio" min="0" max="5" val="0.8"/>
	<attnum name="inertia" val="0.0064"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.983"/>
      </section>
      <section name="5">
	<attnum name="ratio" min="0" max="5" val="0.67"/>
	<attnum name="inertia" val="0.0107"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.948"/>
      </section>
      <section name="6">
	<attnum name="ratio" min="0" max="5" val="0.5"/>
	<attnum name="inertia" val="0.0150"/>
	<attnum name="efficiency" min="0.5" max="1.0" val="0.930"/>
      </section>
    </section>
  </section>

  <section name="Drivetrain">
    <!-- allowed values RWD, FWD, 4WD -->
    <attstr name="type"                 val="4WD"/>
    <!-- drive shaft inertia -->
    <attnum name="inertia" unit="kg.m2" val="0.0091"/>
  </section>

  <section name="Steer">
    <attnum name="steer lock" unit="deg" min="1" max="45" val="45"/>
    <attnum name="max steer speed" unit="deg/s" min="1" max="360" val="120"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.65"/>
    <attnum name="max pressure" unit="kPa" min="100" max="150000" val="10000"/>
  </section>

  <section name="Front Axle">
    <attnum name="xpos" min="0.5" max="2.5" val="1.104"/>
    <attnum name="inertia" unit="kg.m2"        val="0.0056"/>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.012"/>
  </section>

  <section name="Rear Axle">
    <attnum name="xpos" min="-2.5" max="-0.5" val="-1.469"/>
    <attnum name="inertia" unit="kg.m2"        val="0.0080"/>
    <attnum name="roll center height" unit="m" min="0" max="0.5" val="0.04"/>
  </section>

  <section name="Front Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"/>
    <attnum name="inertia" unit="kg.m2" val="0.0488"/>
    <attnum name="ratio" min="0" max="15" val="1.0"/>
    <attnum name="efficiency" min=".5" max="1.0" val="0.9625"/>
  </section>

  <section name="Rear Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"/>
    <attnum name="inertia" unit="kg.m2" val="0.0488"/>
    <attnum name="ratio" min="0" max="15" val="1.0"/>
    <attnum name="efficiency" min=".5" max="1.0" val="0.9625"/>
  </section>

  <section name="Central Differential">
    <!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
    <attstr name="type" in="NONE,SPOOL,FREE,LIMITED SLIP,VISCOUS COUPLER" val="VISCOUS COUPLER"/>
    <attnum name="min torque bias" val="50" unit="%"/>
    <attnum name="max torque bias" val="60" unit="%"/>
    <attnum name="inertia" min="0.001" max="0.1" val="0.0488" unit="kg.m2"/>
    <attnum name="ratio" min="0" max="10" val="4.5"/>
    <attnum name="efficiency" min="0.5" max="1.0" val="0.9625"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ypos" unit="m" max="-0.5" min="-0.9" val="-0.81"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"/>
    <attnum name="inertia" val="1.2200" unit="kg.m2" min="1"/>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"/>
    <!-- initial ride height -->
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="rolling resistance" min="0.005" max="0.1" val="0.0105"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ypos" unit="m" min="0.5" max="0.9" val="0.81"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"/>
    <attnum name="inertia" unit="kg.m2" val="1.2200" min="1"/>
    <attnum name="mu" min="0.05" max="1.6" val="1.4"/>
    <attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
    <attnum name="rolling resistance" min="0.005" max="0.1" val="0.0105"/>
    <attnum name="toe" unit="deg" min="-5" max="5" val="0"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ypos" unit="m" max="-0.5" min="-0.9" val="-0.81"/>
    <attnum name="rim diameter" unit="in" min="16" max="18" val="18.0"/>
    <attnum name="tire width" unit="mm" min="135" max="335" val="335"/>
    <attnum name="tire height-width ratio" min="0.3" max="0.8" val=".30"/>
