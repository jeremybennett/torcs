<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Track-4WD-GrB" type="template">

  <section name="Car">
    <attstr name="category" val="Track-4WD-GrB"/>
    <attnum name="body length" unit="m" min="4.5" max="5.0" val="4.683"/>
    <attnum name="body width" unit="m" min="0.8" max="2.1" val="1.4"/>
    <attnum name="body height" unit="m" min="1.1" max="1.7" val="1.173"/>
    <!-- collision bounding box -->
    <attnum name="overall length" unit="m" min="4.5" max="5.0" val="4.7"/>
    <attnum name="overall width"  unit="m" min="1.2" max="2.1" val="1.7"/>
    <attnum name="mass" unit="kg" min="1100.0" max="2500" val="1550.0"/>
    <attnum name="GC height" unit="m" min="0.15" max="0.5" val="0.30"/>
    <!-- weight bias -->
    <attnum name="front-rear weight repartition" min="0.4" max="0.6" val="0.5"/>
    <attnum name="front right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
    <attnum name="rear right-left weight repartition" min="0.4" max="0.6" val="0.5"/>
    <!-- used for inertia, indicates the good mass centering -->
    <attnum name="mass repartition coefficient" min="0.2" max="1.0" val="0.9"/>
    <attnum name="fuel tank" unit="l" min="30.0" max="100.0" val="100.0"/>
    <attnum name="initial fuel" unit="l" min="1.0" max="100.0" val="5.0"/>
  </section>

  <section name="Aerodynamics">
    <attnum name="Cx" min="0.20" max="2.0" val="0.38"/>
    <attnum name="front area" unit="m2" min="1.0" max="3.0" val="2.085"/>
    <attnum name="front Clift" min="0.0" max="1.0" val="0.3"/>
    <attnum name="rear Clift"  min="0.0" max="1.0" val="0.2"/>
  </section>

  <section name="Front Wing">
    <attnum name="area" unit="m2" min="0" max="0.5" val="0.5"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"/>
    <attnum name="xpos" unit="m" min="1.0" max="2.5" val="2"/>
    <attnum name="zpos" unit="m" min="0.1" max="0.5" val="0.1"/>
  </section>

  <section name="Rear Wing">
    <attnum name="area"  unit="m2" min="0" max="1.0" val="1.0"/>
    <attnum name="angle" unit="deg" min="0" max="30" val="30"/>
    <attnum name="xpos" unit="m" min="-2.5" max="-1.0" val="-2.3"/>
    <attnum name="zpos" unit="m" min="0.1" max="1.5" val="1.1"/>
  </section>

  <!-- Engine Max curve -->
  <section name="Engine">
    <attnum name="inertia" min="0.06" val="0.08" unit="kg.m2"/>
    <attnum name="revs limiter" min="3000" max="9500" unit="rpm" val="9000"/>
    <attnum name="revs maxi" min="3000" max="10000" unit="rpm" val="10000"/>
    <attnum name="tickover" min="500" max="5000" unit="rpm" val="2000"/>
    <attnum name="fuel cons factor" min="1.0" max="2.0" val="1.2"/>
    <attnum name="torque min" val="300" unit="N.m"/>
    <attnum name="torque max" val="750" unit="N.m"/>
    <attnum name="torque def step" unit="rpm" val="500"/>
    <section name="data points">
      <section name="1">
	<attnum name="rpm" unit="rpm" val="0"/>
	<attnum name="Tq" min="0" max="280" unit="N.m" val="361"/>
